script(type="text/template" id="productNewModelTemplate")
    div.parentsInfo.col-lg-2
        div.list-group
            a(data-target-container="infoSubTab").list-group-item.newProductNav.active Information
            a(data-target-container="spacificationSubTab").list-group-item.newProductNav Spacification
            a(data-target-container="pricesSubTab").list-group-item.newProductNav Prices
            a(data-target-container="photosSubTab").list-group-item.newProductNav Photos
            a(data-target-container="discountsSubTab").list-group-item.newProductNav Discounts
            a(data-target-container="stockSubTab").list-group-item.newProductNav Stock
            a(data-target-container="suppliersSubTab").list-group-item.newProductNav Suppliers
            a(data-target-container="ceoSubTab").list-group-item.newProductNav CEO
    div.col-lg-10
        div.infoSubTab.subTab
            div.panel.panel-default
                div.panel-heading
                    h5
                        strong Information
                div.panel-body
                    form.form-horizontal(role="form")
                        div.form-group
                            label.control-label.col-lg-2(for="startDateProduct")
                                a Start date:
                            div.col-lg-3
                                div.input-group.input-group-sm
                                    input.form-control.hasDatePicker#startDateProduct(tepe="text")
                                    label.input-group-addon(for="startDateProduct")
                                        i.fa.fa-calendar.fa-lg
                            p.help-block.text-muted
                                small
                                    i.fa.fa-asterisk
                                    |  required
                        div.form-group
                            label.control-label.col-lg-2(for="endDateProduct")
                                a End date:
                            div.col-lg-3
                                div.input-group.input-group-sm
                                    input.form-control.hasDatePicker#endDateProduct(tepe="text")
                                    label.input-group-addon(for="endDateProduct")
                                        i.fa.fa-calendar.fa-lg
                        legend
                        div.form-group
                            label.control-label.col-lg-2(for="categoryProduct")
                                a Category:
                            div.col-lg-3
                                select#categoryProduct.form-control.input-sm(type="text" name="product_parent_id")
                                    <%_.each(categories, function(category){%>
                                    option(data-id!="<%= category.category_id %>") <%= category.path %>
                                    <%});%>
                            p.help-block.text-muted
                                small
                                    i.fa.fa-asterisk
                                    |  required
                        div.form-group
                            label.control-label.col-lg-2(for="brandProduct")
                                a Brand:
                            div.col-lg-3
                                select#brandProduct.form-control.input-sm(type="text" name="product_brand_id")
                                    <%_.each(brands, function(brand){%>
                                    option(data-id!="<%= brand.brand_id %>") <%= brand.brand_name %>
                                    <%});%>
                            p.help-block.text-muted
                                small
                                    i.fa.fa-asterisk
                                    |  required
                        legend
                        div.form-group
                            label.control-label.col-lg-2(for="shortNameProduct")
                                a Short name:
                            div.col-lg-9
                                input#shortNameProduct.form-control.input-sm(type="text" name="product_short_name")
                            p.help-block.text-muted
                                small
                                    i.fa.fa-asterisk
                                    |  required
                        legend
                        div.form-group
                            label.control-label.col-lg-2(for="fullNameProduct")
                                a Full name:
                            div.col-lg-9
                                input#fullNameProduct.form-control.input-sm(type="text" name="product_full_name")
                            p.help-block.text-muted
                                small
                                    i.fa.fa-asterisk
                                    |  required
                        legend
                        div.form-group
                            label.control-label.col-lg-2(for="shortSprsificationProduct")
                                a Short description:
                            div.col-lg-9
                                textarea#shortSprsificationProduct.form-control.input-sm(name="product_short_description")
                            p.help-block.text-muted
                                small
                                    i.fa.fa-asterisk
                                    |  required
                        div.form-group
                            label.control-label.col-lg-2(for="fullSprsificationProduct")
                                a Full description:
                            div.col-lg-9
                                textarea#fullSprsificationProduct.form-control.input-sm(name="product_full_description")
                            p.help-block.text-muted
                                small
                                    i.fa.fa-asterisk
                                    |  required
                        legend
                        div.form-group
                            label.control-label.col-lg-2(for="europeBarcodeProduct")
                                a Barcode EAN-13(JAN):
                            div.col-lg-9
                                input#europeBarcodeProduct.form-control.input-sm(name="product_barcode_EAN13")
                        div.form-group
                            label.control-label.col-lg-2(for="usaBarcodeProduct")
                                a Barcode UPC:
                            div.col-lg-9
                                input#usaBarcodeProduct.form-control.input-sm(name="product_barcode_UPC")
                div.panel-footer
                    div.text-right
                        button.btn.btn-default.saveNewProductInfo(type="submit") save
        div.spacificationSubTab.savingAlert.hide
            div.panel.panel-default
                div.panel-heading
                    strong Specification
                div.panel-body
                    form(role="form")

        div.pricesSubTab.savingAlert.hide
            div.panel.panel-default
                div.panel-heading
                    strong Prices
                div.panel-body
                    form(role="form")

        div.photosSubTab.savingAlert.hide
            div.panel.panel-default
                div.panel-heading
                    strong.photosSubTabHeader Photos
                        div.pull-right.photosSubTabServiceBtns
                            a.btn.btn-default.btn-xs.dropdown-toggle(title="edit" data-toggle="dropdown")
                                i.fa.fa-gears
                            ul.dropdown-menu(role="menu")
                                li
                                    a
                                        i.fa.fa-times
                                        | delete

                div.panel-body
                    div.col-lg-12
                        div.col-lg-6
                            a.thumbnail
                                img(src="images/temp/slider6.png")
                        div.col-lg-6.smallPhotos
                            div.col-lg-3
                                a.thumbnail
                                    div.checkboxFour
                                        input(type="checkbox" value="1" id="checkboxFourInput" name="")
                                        label(for="checkboxFourInput")
                                    img(src="images/temp/slider6.png")
                            div.col-lg-3
                                a.thumbnail
                                    div.checkboxFour
                                        input(type="checkbox" value="1" id="checkboxFourInput1" name="")
                                        label(for="checkboxFourInput1")
                                    img(src="images/temp/slider6.png")
                            div.col-lg-3
                                a.thumbnail
                                    div.checkboxFour
                                        input(type="checkbox" value="1" id="checkboxFourInput2" name="")
                                        label(for="checkboxFourInput2")
                                    img(src="images/temp/slider6.png")
                            div.col-lg-3
                                a.thumbnail
                                    div.checkboxFour
                                        input(type="checkbox" value="1" id="checkboxFourInput3" name="")
                                        label(for="checkboxFourInput3")
                                    img(src="images/temp/slider6.png")
                            div.col-lg-3
                                a.thumbnail
                                    div.checkboxFour
                                        input(type="checkbox" value="1" id="checkboxFourInput4" name="")
                                        label(for="checkboxFourInput4")
                                    img(src="images/temp/slider6.png")
                            div.col-lg-3
                                a.thumbnail
                                    div.checkboxFour
                                        input(type="checkbox" value="1" id="checkboxFourInput5" name="")
                                        label(for="checkboxFourInput5")
                                    img(src="images/temp/slider6.png")
                            div.col-lg-3
                                a.thumbnail
                                    div.checkboxFour
                                        input(type="checkbox" value="1" id="checkboxFourInput5" name="")
                                        label(for="checkboxFourInput5")
                                    img(src="images/temp/slider6.png")
                            div.col-lg-3
                                a.thumbnail
                                    div.checkboxFour
                                        input(type="checkbox" value="1" id="checkboxFourInput6" name="")
                                        label(for="checkboxFourInput6")
                                    img(src="images/temp/slider6.png")
                    form.col-lg-12.form-horizontal(role="form")
                        div.form-group
                            label.control-label.col-lg-2(for="productNewPhoto")
                                a Select file:
                            div.col-lg-4
                                input#productNewPhoto.input-sm(type="file")
                                div.progress.uploadPhoto
                                    div.progress-bar.progress-bar-success(role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100")


        div.discountsSubTab.savingAlert.hide
            div.panel.panel-default
                div.panel-heading
                    strong Discounts
                div.panel-body
                    form(role="form")
        div.stockSubTab.savingAlert.hide
            div.panel.panel-default
                div.panel-heading
                    strong Stock
                div.panel-body
                    form(role="form")
        div.suppliersSubTab.savingAlert.hide
            div.panel.panel-default
                div.panel-heading
                    strong Suppliers
                div.panel-body
                    form(role="form")
        div.ceoSubTab.savingAlert.hide
            div.panel.panel-default
                div.panel-heading
                    strong CEO
                div.panel-body
                    form(role="form")