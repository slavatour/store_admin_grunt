{
  "name": "store application (admin)",
  "version": "0.0.4",
  "homepage": "https://github.com/slavatour/store-admin-require",
  "authors": [
    "dzvinok.cv@gmail.con"
  ],
  "description": "internet store with admin",
  "main": "app.js",
  "keywords": [
    "internet",
    "store",
    "shop"
  ],
  "exportsOverride": {
    "bootstrap-sass": {
      "js": "dist/js/*min.js",
      "../../stylesheets/libs/scss": "lib/*.scss",
      "../../stylesheets/libs/fonts": "fonts/*"
    },
    "requirejs": {
      "js": "require.js"
    },
    "backbone": {
      "js": "backbone.js"
    },
    "backbone.*": {
      "js": [
        "lib/*.min.js",
        "lib/*.map"
      ]
    },
    "bootstrap-switch": {
      "js": "dist/js/*.min.js",
      "../../stylesheets/libs/css": "dist/css/bootstrap3/*min.css"
    },
    "i18next": {
      "js": "*amd.min.js"
    },
    "jquery": {
      "js": [
        "dist/*min.js",
        "dist/*min.map"
      ]
    },
    "jquery-ui": {
      "js": [
        "jquery-ui.min.js",
        "ui/*.js"
      ]
    },
    "underscore": {
      "js": "underscore.js"
    },
    "chartjs": {
      "js": "Chart.js"
    },
    "moment": {
      "js": "min/moment-with-locales.min.js"
    },
    "backbone-computedfields": {
      "js": "lib/amd/*.min.js"
    },
    "ckeditor": {
        "js": [
            "*.js",
            "*.css"
        ]
    }
  },
  "devDependencies": {
    "backbone": "latest",
    "jquery": "latest",
    "jquery-ui": "latest",
    "backbone.marionette": "latest",
    "underscore": "latest",
    "i18next": "~1.7.2",
    "bootstrap-switch": "latest",
    "bootstrap-sass": "*",
    "chartjs": "~1.0.1",
    "requirejs": "*",
    "moment": "~2.8.3",
    "backbone-computedfields": "~0.0.8",
    "ckeditor": "~4.4.5"
  },
  "license": "free",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
  }
}
